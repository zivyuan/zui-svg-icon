{
  "name": "zui-svg-icon",
  "version": "0.1.8",
  "description": "一款适用于 uni-app 的 SVG 图标组件。除基础的图标显示之外, 还支持单色/多色图标换色。支持 uni-app APP端, h5端和微信小程序",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "zip",
    "svgicon": "node ./uni_modules/zui-svg-icon/tools/generate-svg-icon.js",
    "pub:h5": "rm -rf docs/* && cp -r unpackage/dist/build/h5/* docs/"
  },
  "keywords": [
    "svg",
    "icon",
    "uniapp",
    "svg-icon"
  ],
  "author": "ziv.yuan@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "svgo": "^3.0.2"
  },
  "uni-app": {
  	"scripts": {
  		"mp-dingtalk": {
  		"title":"钉钉小程序",
  			"env": {
  				"UNI_PLATFORM": "mp-alipay"
  			},
  			"define": {
  				"MP-DINGTALK": true
  			}
  		},
  		"mp-dingtalk-dev": {
        "title":"钉钉小程序 (DEV)",
          "env": {
            "UNI_PLATFORM": "mp-alipay"
          },
          "define": {
            "MP-DINGTALK": true,
            "DEVELOPMENT": true
          }
        }
  	}
  }
}
